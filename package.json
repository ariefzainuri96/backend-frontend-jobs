{
  "name": "frontend-jobs",
  "version": "0.0.1",
  "description": "Frontend Jobs Node.js + Express",
  "main": "app.ts",
  "scripts": {
    "test": "npm test",
    "start": "nodemon dist/server/app.js",
    "dev": "ts-node swagger/swagger.ts && nodemon --config nodemon.json server/app.ts",
    "build": "rimraf dist && tsc && ts-node swagger/swagger.ts && copyfiles -u 1 swagger/swagger-output.json dist/swagger && copyfiles -u 1 crt/* dist/crt",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "frontend-jobs"
  ],
  "author": "Arief Zainuri",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.2",
    "mongoose-to-swagger": "^1.5.1",
    "nodemon": "^3.0.3",
    "save-dev": "^0.0.1-security",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.8.10",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}